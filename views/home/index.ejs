<div class="chat-container" ng-controller="chatbox as vm">
    {{ vm.param }}

    <p> <input type="text" ng-model="vm.input"/> </p>
    <p> <input type="submit" value="Send" ng-click="vm.send()" /> </p>
    <hr />

    <ul>
        <li ng-repeat="chat in vm.chats"> {{ chat.message }} </li>
    </ul>

</div>